package change;

import org.eclipse.emf.common.notify.Notification;
import org.eclipse.emf.ecore.EObject;
import org.eclipse.emf.ecore.EReference;

public class SetEReferenceEvent extends EReferenceEvent
{
	/*EObject added to another EObject via some EReference*/
	public SetEReferenceEvent(EObject focusObject,EObject newValue,EReference eReference, NotifierType notifierType)
    {
        super(Event.SET_EREFERENCE_EVENT, focusObject, newValue, eReference,notifierType);
    }
	
	public SetEReferenceEvent(EObject value) //i.e objects added to resource
	{
		super(Event.SET_EREFERENCE_EVENT, value);
	}
    
    public SetEReferenceEvent(Notification n)
    {
       super(Event.SET_EREFERENCE_EVENT,n);
    } 
}


