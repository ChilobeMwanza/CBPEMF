package change;

import java.util.List;

import org.eclipse.emf.ecore.EAttribute;
import org.eclipse.emf.ecore.EObject;

public class SetEAttributeEvent extends Event
{
	 public SetEAttributeEvent(int event_type) {
		super(event_type);
		// TODO Auto-generated constructor stub
	}

	private EObject focus_obj;
	 private EAttribute attr;
	 private List<Object> attr_values_list;
	 
     public SetEAttributeEvent(EObject focus_obj, EAttribute attr, List<Object> attr_values_list)
     {
        super(Event.SET_EATTRIBUTE);
        
        this.focus_obj = focus_obj;
        this.attr = attr;
        this.attr_values_list = attr_values_list;
     }
}
