package change;

import org.eclipse.emf.ecore.EAttribute;
import org.eclipse.emf.ecore.EObject;

public class SetEReferenceSingleEvent extends Event
{
	
	private enum EventType
	{
		SET_A,
		UNSET_A,
		ADD_R,
	    CREATE,
		SET_R,
		DEL_R,
		UNSET_R,
		NULL;
	}
	private EObject added_obj;
	private EAttribute attr;
	
	
}
