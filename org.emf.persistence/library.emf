@namespace(uri="http://org.emf.persistence", prefix="")

package library;

class Library
{
	val Book[*] books;
	val Employee[*] employees;
}

class Book extends NamedElement
{
    ref Author[1]#books author;
	attr double idNumber;
}

class Author extends NamedElement
{
	ref Book[+]# author books;
}

class Employee extends NamedElement
{
	attr EmployeeType employeeType;
}

abstract class NamedElement 
{
	attr String name;
}

enum EmployeeType
{
 temp;
 permananent;
 contract;	
}
