@namespace(uri="http://io.emf.change_based_persistence.text", prefix="cm")

package library;

class Library extends NamedElement
{
	val Book[*] goodBooks;
	val Book[*] badBooks;
	val Book superBook;
	attr int numEmployees;
	attr double aDouble;
	attr String[*] employeeNames;
	attr int [*] numbersList;
	ref Vehicle mainLibraryCar;
	ref Vehicle[2] reserveLibraryCars; 
	
	/*Attr Enums*/
	attr LibraryType libraryTypeAttrSingle;
	attr LibraryType [*] libraryTypeAttrMany;
	attr LibraryType [2] libraryTypeAttrTwo; 
}

class Book extends NamedElement
{
	attr double idNumber;
	val Author anAuthor;
}

class Student extends NamedElement
{
	ref Module [*]#registeredStudents registeredModules;
	val Vehicle registeredCar;
}

class Module extends NamedElement
{
	ref Student[*]#registeredModules registeredStudents;
}

class Author extends NamedElement
{
	
}

class Vehicle extends NamedElement
{
	attr float vehicleID;
}

abstract class NamedElement 
{
	attr String name;
}

enum LibraryType
{
	A;
	B;
	C;
	D;
	E;
	F;
}
